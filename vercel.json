{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": ".output",
  "public": true,
  "functions": {
    "server/api/**": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/server/data/:file*",
      "destination": "/server/data/:file*"
    }
  ]
}
